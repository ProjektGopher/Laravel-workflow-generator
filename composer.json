{
    "name": "projektgopher/laravel_workflow_generator",
    "description": "Creates a make:workflow artisan command to scaffold out a number of useful GitHub actions workflows for Laravel.",
    "keywords": [
        "ProjektGopher",
        "laravel",
        "laravel_workflow_generator"
    ],
    "homepage": "https://github.com/projektgopher/laravel_workflow_generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Len Woodward",
            "email": "Len@ProjektGopher.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Projektgopher\\WorkflowGenerator\\": "src",
            "Projektgopher\\WorkflowGenerator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Projektgopher\\WorkflowGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Projektgopher\\WorkflowGenerator\\WorkflowGeneratorServiceProvider"
            ],
            "aliases": {
                "WorkflowGenerator": "Projektgopher\\WorkflowGenerator\\WorkflowGeneratorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
